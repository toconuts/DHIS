<div class="outbreakitems">
  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th>Age</th>
        <th colspan="2">< 1</th>
        <th colspan="2">1-4</th>
        <th colspan="2">5-14</th>
        <th colspan="2">15-24</th>
        <th colspan="2">25-44</th>
        <th colspan="2">45-64</th>
        <th colspan="2">65+</th>
        <th rowspan="2">Total</th>
      </tr>
      <tr>
        <th>Day of the week</th>
        <th>Fem</th>
        <th>Male</th>
        <th>Fem</th>
        <th>Male</th>
        <th>Fem</th>
        <th>Male</th>
        <th>Fem</th>
        <th>Male</th>
        <th>Fem</th>
        <th>Male</th>
        <th>Fem</th>
        <th>Male</th>
        <th>Fem</th>
        <th>Male</th>
      </tr>
    </thead>

    <tbody>
    {% for item in outbreak.outbreakItems %}
      <tr>
        <td>{{ item.dayOfTheWeekString }}</td>
        <td>{{ item.ageGroup1F }}</td>
        <td>{{ item.ageGroup1M }}</td>
        <td>{{ item.ageGroup2F }}</td>
        <td>{{ item.ageGroup2M }}</td>
        <td>{{ item.ageGroup3F }}</td>
        <td>{{ item.ageGroup3M }}</td>
        <td>{{ item.ageGroup4F }}</td>
        <td>{{ item.ageGroup4M }}</td>
        <td>{{ item.ageGroup5F }}</td>
        <td>{{ item.ageGroup5M }}</td>
        <td>{{ item.ageGroup6F }}</td>
        <td>{{ item.ageGroup6M }}</td>
        <td>{{ item.ageGroup7F }}</td>
        <td>{{ item.ageGroup7M }}</td>
        <td>{{ item.getTotal() }}</td>
      </tr>
    {% endfor %}
    </tbody>

  </table>
</div>